{
//  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [ "ESNext", "dom" ],
    "allowImportingTsExtensions": true,
    "types": [
      "@cloudflare/workers-types/2023-07-01"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
}